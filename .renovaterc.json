{
  "extends": [
    ":semanticCommits",
    ":semanticPrefixFix"
  ],
  "ignorePaths": [
    ".github/workflows/build.yml"
  ],
  "labels": [
    "maintenance"
  ],
  "lockFileMaintenance": {
    "automerge": true,
    "enabled": true,
    "semanticCommitType": "chore"
  },
  "packageRules": [
    {
      "automerge": true,
      "matchCurrentVersion": ">=1.0.0",
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ],
  "semanticCommitScope": null
}
